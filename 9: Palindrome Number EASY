class Solution {
public:
    bool isPalindrome(int x) {
        long long res = 0;
        int temp = x;
        
        while (temp > 0) {
            res = res * 10 + temp % 10;
            temp /= 10;
        }
        
        return res == x;
    }
}; O(N) time O(1) space

const isPalindrome = x => {
  let res = 0;
  let temp = x;
  
   while (temp > 0) {
    res = res * 10 + temp % 10;
    temp = Math.floor(temp/10);
   }
  
  return res == x;
}

